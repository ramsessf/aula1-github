package curso_programacao;

import java.util.Scanner;

public class Matriz {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Digite a ordem da matiz: ");
		int N = sc.nextInt();
		
		int [][] matriz = new int [N][N];
		
		for(int i = 1; i <= N; i++) {
			for(int j = 1; j <= N; j++) {
				
				matriz[i][j] = sc.nextInt();
			}
		}
		
		sc.close();

	}

}
