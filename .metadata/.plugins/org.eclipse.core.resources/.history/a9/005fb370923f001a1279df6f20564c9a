package curso_programacao;

import java.text.ParseException;
import java.text.SimpleDateFormat;


public class Date {

	public static void main(String[] args) throws ParseException {
		
		SimpleDateFormat sdf1 = new SimpleDateFormat("25/01/2020");
		SimpleDateFormat sdf2 = new SimpleDateFormat("25/01/2020 13:27:00");
		
		
		java.util.Date y1 = sdf1.parse("25/01/2020");
		java.util.Date y2 = sdf2.parse("25/01/2020 13:27:00");
		
		System.out.println(y1);

	}

}
